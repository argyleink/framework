{
  "name": "famous-framework",
  "version": "0.10.0",
  "description": "A modern take on building reusable components and maintainable applications",
  "keywords": [
    "famous",
    "framework"
  ],
  "license": "MIT",
  "repository": "Famous/framework",
  "bugs": "https://github.com/Famous/framework/issues",
  "private": true,
  "authors": [
    "Zack Brown <zack@famo.us>",
    "Matthew Trost <matthew@famo.us>",
    "Arkady Pevzner <arkady@famo.us>",
    "Imtiaz Majeed <imtiaz@famo.us>",
    "Jordan Papaleo <jordan@famo.us>",
    "Larry Gordon <lg@famo.us>"
  ],
  "scripts": {
    "dev": "npm run watch-runtime & npm run bootstrap",
    "bootstrap": "rm -rf $PWD/dev/workspace/v1 && node dev/dev.js local-only-bootstrap --sourceDirectory=$PWD/lib/core-components --blocksDirectory=$PWD/dev/workspace --port=1618",
    "build-runtime": "node dev/build-runtime.js build-runtime",
    "watch-runtime": "watchify lib/index.js -t [ envify --FF_ASSET_READ_HOST http://localhost:1618 ] -o dev/workspace/famous-framework.bundle.js --debug --verbose",
    "develop-component": "node dev/dev.js develop-component --blocksDirectory=$PWD/dev/workspace --port=1618",
    "publish-component": "node dev/dev.js publish-component --blocksDirectory=$PWD/dev/workspace",
    "test": "node dev/test.js",
    "lint": "eslint --ignore-path .eslintignore ."
  },
  "dependencies": {
    "async": "1.2.1",
    "babel": "5.5.8",
    "browserify": "10.2.4",
    "chalk": "1.0.0",
    "chokidar": "1.0.3",
    "clone": "1.0.2",
    "commander": "2.8.1",
    "envify": "3.4.0",
    "escodegen": "1.6.1",
    "esprima": "2.2.0",
    "estraverse": "4.1.0",
    "express": "4.12.4",
    "famous": "Famous/engine#v0.6.0",
    "form-data": "matthewtoast/node-form-data#v0.2.1",
    "jsdom": "3.1.2",
    "less": "2.5.1",
    "livereload": "0.3.7",
    "lodash": "3.9.3",
    "mkdirp": "0.5.1",
    "node-watch": "0.3.4",
    "nodemon": "1.3.7",
    "recursive-readdir": "1.2.1",
    "request": "2.57.0",
    "serve": "1.4.0",
    "stylus": "0.51.1",
    "temp": "0.8.3",
    "traverse": "0.6.6",
    "watchify": "3.2.2",
    "xhr": "2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "3.1.15",
    "eslint": "0.23.0",
    "jsdoc": "3.3.2",
    "smokestack": "3.3.0",
    "tap-closer": "1.0.0",
    "tape": "4.0.0"
  }
}
